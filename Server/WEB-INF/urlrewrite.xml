<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <!-- Rule to serve index.html for any path under /eventlogs -->
    <rule>
        <name>Ember SPA Routing</name>
        <from>^/eventlogs/(.*)$</from>
        <to>/index.html</to>
    </rule>
</urlrewrite>
